{
  "name": "printio",
  "version": "0.0.0",
  "scripts": {
    "build": "webpack -p",
    "precommit": "run-s format:fix lint",
    "lint": "tslint --project \"tsconfig.json\"",
    "format:fix": "pretty-quick --staged",
    "format:check": "prettier --config ./.prettierrc --list-different \"src/**/*{.ts,.js,.json,.html,.css,.scss}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s precommit"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "husky": "^1.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "ts-loader": "^5.3.3",
    "tslint": "^5.13.1",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3"
  }
}
